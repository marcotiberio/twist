<body>
  <a href="#mainContent" class="skipLink">
    {{ __("Skip to main content", "flynt") }}
  </a>
  {% block layout %}
  <div class="pageWrapper">
    <header class="mainHeader" id="mainHeader">
      {% block header %}
        {% if fn('is_front_page') %}
          {{ renderComponent('BlockAnnouncement') }}
        {% endif %}
        {{ renderComponent('NavigationMain') }}
        {{ renderComponent('NavigationBurger') }}
      {% endblock %}
    </header>
    <main id="mainContent" class="mainContent" aria-label="{{ __('Content', 'flynt') }}">
      {% if function('post_password_required') %}
      {{ function('get_the_password_form') }}
      {% else %}
      {% block content %}
      No <code>{{ '{% block content %}' }}</code> defined for this template.
      {% endblock %}
      {% endif %}
    </main>
    <footer class="mainFooter">
      {% block footer %}
      {{ renderComponent('BlockContact') }}
      {{ renderComponent('NavigationFooterColumns') }}
      {% endblock %}
    </footer>
  </div>
  {% endblock %}
  <div class="wpFooter">
    {{ function('wp_footer') }}
  </div>
</body>

</html>
