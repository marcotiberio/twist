<div class="flyntComponent {{ options.theme }}" is="flynt-block-landing">
  <div class="container">
    <div class="logo">
      <img class="logo-image" src="{{ logo.src }}" alt="{{ logo.alt }}">
    </div>
    <div class="content">
      {{ contentHtml|e('wp_kses_post')}}
    {% if buttonLink.url %}
      <a href="{{ buttonLink.url }}"{% if buttonLink.target %} target="{{ buttonLink.target }}" rel="noreferrer noopener"{% endif %} class="button button--red">
        {{ buttonLink.title|e }}
      </a>
    {% endif %}
  </div>
  <div class="text-bottom">
    <span class="h3">{{ textBottom }}</span>
    <span class="h3">{{ textBottom }}</span>
    <span class="h3">{{ textBottom }}</span>
    <span class="h3">{{ textBottom }}</span>
    <span class="h3">{{ textBottom }}</span>
    <span class="h3">{{ textBottom }}</span>
    <span class="h3">{{ textBottom }}</span>
    <span class="h3">{{ textBottom }}</span>
    <span class="h3">{{ textBottom }}</span>
    <span class="h3">{{ textBottom }}</span>
  </div>
  <div class="text-left">
    <span class="h3">{{ textLeft }}</span>
    <span class="h3">{{ textLeft }}</span>
    <span class="h3">{{ textLeft }}</span>
    <span class="h3">{{ textLeft }}</span>
    <span class="h3">{{ textLeft }}</span>
    <span class="h3">{{ textLeft }}</span>
    <span class="h3">{{ textLeft }}</span>
    <span class="h3">{{ textLeft }}</span>
    <span class="h3">{{ textLeft }}</span>
    <span class="h3">{{ textLeft }}</span>
  </div>
</div>
